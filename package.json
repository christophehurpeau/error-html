{
  "name": "error-html",
  "version": "0.3.0",
  "description": "render error stack in html",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": "git@github.com:christophehurpeau/error-html.git",
  "homepage": "https://github.com/christophehurpeau/error-html",
  "main": "./index.js",
  "module": "./lib-module/index.js",
  "module-dev": "./lib-module-dev/index.js",
  "browser": "./lib-browsers/index.js",
  "browser-dev": "./lib-browsers-dev/index.js",
  "module:node": "./lib-module-node8/index.js",
  "module:node-dev": "./lib-module-node8-dev/index.js",
  "module:browser": "./lib-module/index.js",
  "module:browser-dev": "./lib-module-dev/index.js",
  "module:modern-browsers": "./lib-module-modern-browsers/index.js",
  "module:modern-browsers-dev": "./lib-module-modern-browsers-dev/index.js",
  "scripts": {
    "build": "pob-build",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "generate:api": "rm -Rf docs/ ; mkdir docs/ ; pob-build doc ; jsdoc README.md lib-doc --recurse --destination docs/ --configure jsdoc.conf.json ; rm -Rf lib-doc",
    "generate:docs": "yarn run generate:api",
    "lint": "eslint --ext .js,.jsx src/",
    "postcheckout": "./.git-hooks/post-checkout",
    "postmerge": "./.git-hooks/post-merge",
    "precommit": "./.git-hooks/pre-commit",
    "preparecommitmsg": "./.git-hooks/prepare-commit-msg ${GIT_PARAMS}",
    "preversion": "yarn run lint && yarn run build && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "version": "pob-version",
    "watch": "pob-watch"
  },
  "lint-staged": {
    "package.json": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.json": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "eslint --fix --quiet",
      "git add"
    ]
  },
  "dependencies": {
    "error-stack-parser": "^2.0.0",
    "escape-html": "^1.0.3",
    "eshighlight-fb": "^1.0.0",
    "flow-runtime": "^0.10.0",
    "source-map": "^0.5.6",
    "source-map-support": "^0.4.14"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-add-jsdoc-annotations": "^5.1.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-jsdoc": "^0.4.0",
    "babel-preset-latest-node": "^0.3.1",
    "babel-preset-modern-browsers": "^9.0.2",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-pob": "^14.1.2",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.1.2",
    "husky": "^0.14.3",
    "jsdoc": "^3.4.1",
    "komet": "^0.1.4",
    "komet-karma": "^0.2.5",
    "lint-staged": "^4.0.2",
    "minami": "^1.1.1",
    "pob-babel": "^17.6.0",
    "pob-release": "^3.1.0",
    "prettier": "^1.5.3"
  }
}
